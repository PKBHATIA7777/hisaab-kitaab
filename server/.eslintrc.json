{
    "env": {
      "node": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
      "no-console": "warn",
      "eqeqeq": "error",
      "curly": "error"
    },
    "ignorePatterns": ["node_modules/", "client/js/lib/"]
  }